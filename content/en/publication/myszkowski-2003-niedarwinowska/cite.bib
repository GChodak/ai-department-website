 author = {Myszkowski, Paweł B},
 journal = {Prace Naukowe Akademii Ekonomicznej we Wrocławiu},
 number = {975 Pozyskiwanie wiedzy i zarządzanie wiedzą},
 pages = {322--331},
 pub_year = {2003},
 title = {Niedarwinowska ewolucja w rozwiązywaniu problemu harmonogramowania},
 venue = {Prace Naukowe Akademii Ekonomicznej we …}
}

