@article{sas2003problemy,
 abstract = {Systemy informatyczne wykorzystujące urządzenia przenośne typu palmtop lub handheld w znacznym stopniu upraszczają dostęp do danych w zastosowaniach wymagających mobilności, takich jak medycyna, handel itp. Na rynku oprogramowania dla wspomnianych urządzeń dominują jednak systemy działające w oparciu o replikacje danych, a stosunkowo niewiele rozwiązań oferuje natychmiastowy dostęp do bazy danych. Przyczyną jest prawdopodobnie brak interfejsów do typowych serwerów baz danych dla urządzeń},
 author = {Sas, J and Radli\ŉski, P},
 journal = {Zeszyty Naukowe Wydziału ETI Politechniki Gda\ŉskiej. Technologie Informacyjne},
 number = {2},
 pages = {423--430},
 pub_year = {2003},
 title = {Problemy dostępu do danych z urządze\ŉ typu palmtop z transmisją danych GPRS/WLAN na przykładzie medycznego systemu informacyjnego},
 venue = {Zeszyty Naukowe Wydziału ETI Politechniki Gdańskiej …}
